<!-- CREDENCIAL DATA -->
<div class="modal fade" id="user_data" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<!--<div class="modal-header">
        <%= image_tag "titulo_wf.jpg", :width => "100%", :alt => "logo" %>
      </div>-->
			<div class="modal-body bg-credencial">
				<div class="row">
					<div class="col-md-6">
            <%= image_tag "logo_cuadrado.png", :width => "70px", :alt => "logo", :class => "pull-right" %>
						<h4>
						<strong>Contrato:</strong>
						<%= @user.contract %>
						</h4>
						<h4>
						<strong>Nº Socio</strong>
						<%= @user.id %>
						</h4>
						<h4>
						<strong>Rut:</strong>
						<%= @user.pid %>
						</h4>
						<h4>
						<strong>Nombre:</strong>
						<%= @user.name %>
						</h4>
						<h4>
						<strong>Teléfono:</strong>
						<%= @user.phone %>
						</h4>
            <h4>
						<strong>Empresa:</strong>
              <%= image_tag @user.logo.url(:thumb), :width => "140px" %>
						</h4>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn bgc-yellow btn-sq" data-dismiss="modal">
          <span class="fa fa-remove fa-fw"></span> CERRAR
        </button>
			</div>
		</div>
	</div>
</div>


